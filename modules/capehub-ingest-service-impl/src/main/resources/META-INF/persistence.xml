<?xml version="1.0" encoding="UTF-8"?>
<persistence
  version="2.0"
  xmlns="http://java.sun.com/xml/ns/persistence" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
   http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="com.capestartproject.ingest.endpoint" transaction-type="RESOURCE_LOCAL">
    <class>com.capestartproject.ingest.endpoint.UploadJob</class>
    <properties>
      <property name="eclipselink.create-ddl-jdbc-file-name" value="create-capehub-ingest-service-impl.jdbc"/>
      <property name="eclipselink.drop-ddl-jdbc-file-name" value="drop-capehub-ingest-service-impl.jdbc"/>
    </properties>
  </persistence-unit>
</persistence>
